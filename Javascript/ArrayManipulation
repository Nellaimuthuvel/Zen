var output = [];

function outputArray(inputArray){
    inputArray.forEach(element =>{
    dict = {
            id : element.id,
            title : element.title,
            childrens : [],
            };
    output.push(dict);
    if(element.childrens.length > 0){
            outputArray(element.childrens);
    }        
    })
}

outputArray(input);
